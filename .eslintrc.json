{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}