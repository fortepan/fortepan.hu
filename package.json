{
  "name": "fortepan",
  "version": "1.0.0",
  "description": "Fortepan.hu public dev repository",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf _dist src/data/manifest-js.json src/data/manifest-css.json",
    "import-autocomplete-data": "node scripts/import-autocomplete-data",
    "import-rss-data": "node scripts/import-rss-data",
    "eleventy": "npx eleventy",
    "eleventy:serve": "npx eleventy --serve",
    "webpack": "webpack",
    "webpack:serve": "webpack -- --watch",
    "postcss": "postcss 'src/scss/styles.scss' --verbose --dir _dist/css/ --ext css",
    "postcss:serve": "npm run postcss -- --watch",
    "tinacms": "npx tinacms build",
    "tinacms:serve": "npx tinacms dev",
    "build": "ENV=production npm-run-all clean import-autocomplete-data import-rss-data webpack postcss eleventy",
    "dev": "ENV=development npm-run-all -l --silent clean import-autocomplete-data import-rss-data webpack postcss --parallel *:serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fortepan/fortepan.hu.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fortepan/fortepan.hu/issues"
  },
  "homepage": "https://github.com/fortepan/fortepan.hu#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@csstools/postcss-sass": "^5.0.1",
    "@elastic/elasticsearch": "^7.10.0",
    "autoprefixer": "10.1.0",
    "caniuse-lite": "^1.0.30001171",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "glob": "^7.1.6",
    "html-minifier": "4.0.0",
    "latest-version": "^6.0.0",
    "netlify-cli": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-hash": "^3.0.0",
    "postcss-import": "^15.0.0",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-inline-svg": "^5.0.0",
    "postcss-minify": "^1.1.0",
    "postcss-scss": "^4.0.5",
    "prettier": "^1.19.1",
    "stylelint": "^15.3.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-prettier": "^1.1.2",
    "terser": "^4.7.0",
    "terser-webpack-plugin": "5.0.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^5.0.1",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@googlemaps/markerclusterer": "^2.5.3",
    "@hotwired/stimulus": "^3.2.1",
    "@hotwired/stimulus-webpack-helpers": "^1.0.1",
    "@tinacms/cli": "^1.4.0",
    "lodash": "^4.17.15",
    "markdown-it": "^13.0.1",
    "rss-parser": "^3.13.0",
    "tinacms": "^1.4.0"
  }
}
